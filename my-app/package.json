{
  "name": "odysseus-identity-manager-client",
  "version": "0.0.1",
  "scripts": {
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
    "vite-workaround": "node _helper/vite-workaround.js",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "postinstall": "npm run prepare"
  },
  "devDependencies": {
    "@material/icon-button": "^11.0.0",
    "@material/textfield": "^11.0.0",
    "@material/theme": "^11.0.0",
    "@material/top-app-bar": "^11.0.0",
    "@material/typography": "^11.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "normalize.css": "^8.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.35.1",
    "smui-theme": "^4.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-forms": "^1.1.51",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@smui/banner": "^4.2.0",
    "@smui/button": "^4.2.0",
    "@smui/common": "^4.2.0",
    "@smui/drawer": "^4.2.0",
    "@smui/icon-button": "^4.2.0",
    "@smui/paper": "^4.2.0",
    "@smui/ripple": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@smui/top-app-bar": "^4.2.0",
    "cookie": "^0.4.1",
    "nanoid": "^3.1.23"
  }
}
